<section>
    <h2>Cards</h2>
    <table>
        {{#cards}}
            <tr>
                <td><a href="{{url}}">{{{name}}}</a></td>
                <td class="n">{{n_decks}}</td>
                <td class="n">{{n_maindecks}}</td>
                <td class="n">{{n_sideboards}}</td>
                <td class="n">{{count_decks}}</td>
                <td class="n">{{count_maindecks}}</td>
                <td class="n">{{count_sideboards}}</td>
            </tr>
        {{/cards}}
    </table>
</section>
